generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cup {
  id   String @id @default(uuid())
  size Int    @unique

  @@map("cups")
}

model User {
  id    String @id @default(uuid())
  name  String
  phone String @unique

  @@map("users")
}
